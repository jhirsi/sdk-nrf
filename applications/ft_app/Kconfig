config FTA_IPERF3
	bool  "Enable FTA iperf3 support"
    default n
	# Heap requirements
	depends on (HEAP_MEM_POOL_SIZE >= 16384)     
    help
        "Not yet supported"

config FTA_SOCKET
	bool  "Enable FTA socket tool support"
    default n
    help
        "Not yet supported"

menu "Zephyr Kernel"
source "Kconfig.zephyr"
endmenu