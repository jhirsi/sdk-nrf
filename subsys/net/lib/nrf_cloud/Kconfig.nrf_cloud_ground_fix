config NRF_CLOUD_CELL_POS
	bool "Enable nRF Cloud Cellular Positioning over MQTT via Ground Fix"
	select NRF_CLOUD_GROUND_FIX

config NRF_CLOUD_GROUND_FIX
	bool "Enable nRF Cloud Ground Fix over MQTT"
	imply FPU
	depends on MODEM_INFO
	depends on MODEM_INFO_ADD_NETWORK
	depends on NRF_CLOUD_MQTT
	select CJSON_LIB
